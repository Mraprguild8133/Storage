# Create .env file
cat > .env << EOL
# Telegram API Configuration
API_ID=your_telegram_api_id_here
API_HASH=your_telegram_api_hash_here
BOT_TOKEN=your_bot_token_here

# Wasabi Cloud Storage Configuration
WASABI_ACCESS_KEY=your_wasabi_access_key_here
WASABI_SECRET_KEY=your_wasabi_secret_key_here
WASABI_BUCKET=your_bucket_name_here
WASABI_REGION=us-east-1
EOL
